<Window x:Class="contactManager.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:contactManager"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel Margin="10">
        <WrapPanel HorizontalAlignment="Center" DockPanel.Dock="Right" Margin="0,0,0,10" Width="185">
            <Button x:Name="btnImportContacts" Click="BtnImportContacts_Click" Content="Import Contacts" Width="150" Margin="10"/>
            <Button x:Name="btnClearLog" Click="BtnClearLog_Click" Content="Clear Log" Width="150" Margin="10"/>
            <Button x:Name="btnPrintDatabase" Click="BtnPrintDatabase_Click" Content="Print Database" Width="150" Margin="10"/>
            <Button x:Name="btnClose" Click="BtnClose_Click" Content="Close App" Width="150" Margin="10"/>
        </WrapPanel>
        <TextBox x:Name="log" Width="568" />
    </DockPanel>
</Window>
